package javaapplication6;

/**
 *
 * @author ThinkPro
 */
public class Newspapers extends Media {
    String type;
    
    public Newspapers(String name, String type){
        super(name);
        this.type = type;
    }
    
    public String getType(){
        return type;
    }
}
